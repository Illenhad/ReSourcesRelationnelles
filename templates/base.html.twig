<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}ResourceRelationnelle{% endblock %}</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
    {% block stylesheets %}{% endblock %}
    <link href="{{ asset('css/header.css') }}" rel="stylesheet"/>
    <link href="{{ asset('css/button.css') }}" rel="stylesheet"/>
</head>
<body>
{% block header %}
    <header>
        <div class="header-content">
            <div id="logo"></div>
            <ul id="nav">
                <li class="nav-item">
                    <a class="nav-link home" aria-current="page" href="{{ path('resources') }}">Accueil</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('user_dashboard') }}">Espace personnel</a>
                </li>
            </ul>
            <div id="search">
                <form class="d-flex" method="get" action="{{ path('resources') }}">
                    <input class="form-control me-2" type="search" placeholder="Ex : Comment devenir un jedi"
                           aria-label="Recherche" name="search"/>
                    <input class="btn btn-blue" type="submit" value="Recherche"/>
                </form>
            </div>
            <div id="connexion">
                <a class="btn btn-primary btn-conexion" href="{{ path('login') }}">Connexion</a>
            </div>
        </div>
    </header>
{% endblock %}
{#<button class="btn btn-primary">PRIMARY</button>#}
{#<button class="btn btn-blue">BLUE</button>#}
{#<button class="btn btn-green">GREEN</button>#}
{#<button class="btn btn-red">RED</button>#}
{#<button class="btn btn-yellow">YELLOW</button>#}
{% block body %}{% endblock %}
